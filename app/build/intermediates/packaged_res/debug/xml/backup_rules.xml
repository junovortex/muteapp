<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Include all SharedPreferences first -->
    <include domain="sharedpref" path="." />
    
    <!-- Then exclude SharedPreferences that contain sensitive data -->
    <exclude domain="sharedpref" path="MuteAppPrefs.xml" />
    
    <!-- Include other app data -->
    <include domain="file" path="." />
</full-backup-content>
